
<% @travels = [ @travel ]
   content_for :map_overlay do %>

  <%= render template: "travels/index" %>

  <% if @options[:mock].blank? -%>
    <div ng-controller="TrackingServiceController" ng-init="track(<%= @travel.performer.id %>, <%= @travel.id %>)"></div>
  <% else -%>
    <div ng-controller="TrackingServiceController" ng-init="fakeTrack(<%= @travel.performer.id %>, <%= @travel.id %>, '<%= @travel.route.polyline %>')"></div>
  <% end -%>

<% end %>