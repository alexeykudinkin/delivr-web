<%= form.fields_for(:origin, Travels::Places::Origin.new) do |subform|      %>

  <%= subform.label      :origin                                            %>
  <%= subform.text_field :address,      placeholder:  'A',
                                        data: { tracking_target: 'origin' } %>
  <%= subform.text_field :coordinates,  type:         'hidden'              %>

<% end %>